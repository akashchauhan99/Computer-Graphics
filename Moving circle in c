//Moving circle in c
#include<stdio.h>
#include<conio.h>
#include<graphics.h>
#include<dos.h>
#include<stdlib.h>
void main(){
int gd=DETECT,gm;
int i=0;
initgraph(&gd,&gm,"c:\\TURBOC3\\BGI");
setcolor(BLUE);
for(i; i<300; i++){
circle(100+i,100+i,50);
delay(100);
cleardevice();
}
getch();
closegraph();
}
